{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Debug.js",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/debug.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Current File",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}"
    }
  ]
}
